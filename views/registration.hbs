<h1>Registration Page</h1>

<p>{{message}}</p>

<form method="POST">
    <label for="first-name">First Name:</label>
    <input type="text" id="first-name-field-registration" name="firstname"><br>
    <br>
    <label for="last-name">Last Name:</label>
    <input type="text" id="last-name-field-registration" name="lastname"><br>
    <br>
    <label for="radio-callsign">Radio Callsign:</label>
    <input type="text" id="radio-callsign-field-registration" name="radiocallsign"><br>
    <br>
    <label for="username">Username:</label>
    <input type="text" id="username-field-registration" name="username"><br>
    <br>
    <label for="password">Password:</label>
    <input type="password" id="password-field-registration" name="password"><br>
    <br>
    <label for="confirm-password">Confirm Password:</label>
    <input type="password" id="confirm-password-field-registration" name="confirmpassword"><br>
    <br>
    <label for "email">Email Address:</label>
    <input type="text" id="email-field-registration" name="email"><br>
    <br>
    <label for "access-key">Access Key:</label>
    <input type="password" id="access-key-field-registration" name="key"><br>
    <br>
    <input type="submit" id="submit-registration" value="Submit">
</form>